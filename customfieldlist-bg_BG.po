msgstr "възходящо"

#: D:\xampp\htdocs\wp32en\wp-content\plugins\custom-field-list-widget/widget_custom_field_list.php:1553
msgid "descending (DESC)"
msgstr "низходящо"

#: D:\xampp\htdocs\wp32en\wp-content\plugins\custom-field-list-widget/widget_custom_field_list.php:1555
